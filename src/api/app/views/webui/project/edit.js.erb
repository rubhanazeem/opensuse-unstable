$('#flash').empty();
$('.in-place-editing').animate({
  opacity: 0.25
}, 400, function() {
  scrollToInPlace();
  $('.in-place-editing').html("<%= escape_javascript(render(partial: 'webui/project/edit', locals: { project: @project })) %>");
  $('.in-place-editing').animate({ opacity: 1 }, 400, function() {
    $("form *[autofocus='autofocus']").focus();
  });
});
